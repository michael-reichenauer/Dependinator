<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <AssemblyName>Dependinator.Web</AssemblyName>
    <RootNamespace>Dependinator.Web</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MudBlazor" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Dependinator\Dependinator.csproj" />
    <ProjectReference Include="..\DependinatorCore\DependinatorCore.csproj" />
    <ProjectReference Include="..\DependinatorRoslyn\DependinatorRoslyn.csproj" />
  </ItemGroup>
</Project>
