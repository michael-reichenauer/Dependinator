@namespace DependinatorLib.Areas.Canvas

@inject DependinatorLib.Areas.Canvas.ICanvasService canvasService

<svg xmlns="http://www.w3.org/2000/svg" width="300" height="800" viewBox="0 0 300 800" @onmousemove=@(e =>
    canvasService.MouseMove(this, e)) @onmouseup=@(e => canvasService.MouseUp(this, e))>
    @((MarkupString)canvasService.Get())
</svg>